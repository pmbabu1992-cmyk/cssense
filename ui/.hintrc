{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off",
        "aria-progressbar-name": "off",
        "aria-required-parent": "off",
        "aria-valid-attr-value": "off"
      }
    ],
    "button-type": "off",
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ],
    "disown-opener": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "font-smooth",
          "-ms-filter",
          "speak",
          "speak-as",
          "backdrop-filter"
        ]
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ]
  }
}